{
  "version": 2,
  "builds": [
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/now-builder",
      "config": {}
    }
  ],
  "headers": [
    {
      "source": "(.*)",
      "headers": [{
        "key": "Content-Security-Policy",
        "value": "frame-ancestors http://app.storyblok.com https://app.storyblok.com;"
      }]
    }
  ]
}
